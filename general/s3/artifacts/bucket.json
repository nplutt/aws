{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "This cloudformation template creates an s3 bucket for build artifacts",
    "Resources": {
        "artifacts": {
            "Properties": {
                "BucketName": "artifacts",
                "VersioningConfiguration": {
                    "Status": "Enabled"
                }
            },
            "Type": "AWS::S3::Bucket"
        }
    }
}