{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "This CloudFormation template creates a cloudfront distribution for a static website hosted out of s3.",
    "Resources": {
        "generaldistribution": {
            "Properties": {
                "DistributionConfig": {
                    "Aliases": [],
                    "DefaultCacheBehavior": {
                        "AllowedMethods": [
                            "HEAD",
                            "GET"
                        ],
                        "Compress": "true",
                        "DefaultTTL": 86400,
                        "ForwardedValues": {
                            "QueryString": "false"
                        },
                        "MaxTTL": 31536000,
                        "MinTTL": 0,
                        "TargetOriginId": "origin_1",
                        "ViewerProtocolPolicy": "redirect-to-https"
                    },
                    "DefaultRootObject": "index.html",
                    "Enabled": "true",
                    "HttpVersion": "http2",
                    "Origins": [
                        {
                            "DomainName": "bucket.s3.amazonaws.com",
                            "Id": "origin_1",
                            "S3OriginConfig": {}
                        }
                    ],
                    "PriceClass": "PriceClass_100",
                    "ViewerCertificate": {
                        "AcmCertificateArn": "",
                        "MinimumProtocolVersion": "TLSv1.1_2016",
                        "SslSupportMethod": "sni-only"
                    }
                }
            },
            "Type": "AWS::CloudFront::Distribution"
        }
    }
}